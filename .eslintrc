{
  "env": {
    "browser": true,
    "jest": true,
    "node": true,
    "es6": true
  },

  "extends": [
    "eslint-config-standard"
  ],

  "plugins": [
    "standard"
  ],

  "rules": {
    "brace-style": [2, "stroustrup"],
    "comma-dangle": 0,
    "import/extensions": 1,
    "import/no-unresolved": 2,
    "jsx-quotes": 0,
    "object-curly-spacing": [2, "never"]
  }
}
